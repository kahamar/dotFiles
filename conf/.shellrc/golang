# GO LANG ######################################################################
if command -v go >/dev/null; then
  export GOPATH=~/.golang
  export PATH=$GOPATH/bin:$PATH
	alias go-update='go get -u'
	alias go-update-all='go get -u all'
  if [[ ! -d $GOPATH ]]; then
    mkdir $GOPATH
  fi
	export GO15VENDOREXPERIMENT=1
fi
